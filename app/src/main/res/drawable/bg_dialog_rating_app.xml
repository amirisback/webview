<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="-150dp"
        android:right="-150dp"
        android:top="-200dp">
        <shape android:shape="rectangle">
            <size
                android:width="320dp"
                android:height="320dp" />
            <solid android:color="#4DF6F0F1" />
            <stroke
                android:width="4dp"
                android:color="@color/frogoColorWhite"
                android:dashWidth="4dp"
                android:dashGap="4dp" />
        </shape>
    </item>
</layer-list>